#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    char ch[100];
    int i,count;
    gets(ch);
    i=0;
    for(i=0,count=0;ch[i]!='\0';i++)
    {
        if(ch[i]=='A' || ch[i]=='E' || ch[i]=='I' || ch[i]=='O' || ch[i]=='U' || ch[i]=='a' || ch[i]=='e' || ch[i]=='i' || ch[i]=='o' || ch[i]=='u')
        {
            count++;
        }
        
    }
    printf("%d",count);
    return 0;
}
