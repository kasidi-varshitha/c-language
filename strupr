#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    char ch[100];
    gets(ch);
    int i;
    i=0;
    while(ch[i]!='\0')
    {
        if(ch[i]>='a' && ch[i]<='z')
        {
            ch[i]=ch[i]-32;
        }
        i++;
    }
    printf("%s",ch);
    return 0;
}
